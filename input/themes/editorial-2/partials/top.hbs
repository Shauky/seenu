   <div id="wrapper">
      <div id="main">
         <div class="inner">
            
            {{!-- header --}}
            <header id="header">
               <style type="text/css">
                  /* div */
                  /* Auto layout */
                  #header {
                     display: flex;
                     flex-direction: row;
                     align-items: center;
                     justify-content: space-between;
                     padding: 25px 0px 35px 42px;
                     position: absolute;
                     max-width: 1160px;
                     margin-left:auto;
                     margin-right:auto;
                     height: 0 auto;
                     left: calc(50% - 1205px/2 - 1.55px);
                     top: -3px;
                     
                  }

                  .SN_site_HeaderL {
                     display: flex;
                     align-items: flex-end;
                  }

                  .SN_site_HeaderR {
                        display: flex;
                        align-items: center;
                        justify-content: flex-end;
                  }

                  .SN_site_Header .SN_site_HeaderLink:hover {
                        opacity:.6;
                  }

                  .SN_site_HeaderLink {
                        float: right;
                        margin-right: 24px;
                        font-family: 'Roboto Condensed', 'Asap Condensed', 'neue-haas-unica', sans-serif;
                        font-size: 14px;
                        font-weight: 600;
                        text-transform: uppercase;
                        cursor: pointer;
                  }


                  .logo{
                     /* SeenuPost */
                     width: 613px;
                     height: 31px;
                     font-family: 'Roboto Condensed';
                     font-style: normal;
                     font-weight: 700;
                     font-size: 40px;
                     line-height: 47px;
                     display: flex;
                     align-items: center;
                     text-transform: capitalize;
                     text-decoration: none;
                     color: #000000;
                     /* Inside auto layout */
                     flex: none;
                     order: 0;
                     flex-grow: 0;
                  }

                  .icons{
                     /* SeenuPost */
                     font-family: 'Roboto Condensed';
                     font-style: normal;
                     font-weight: 700;
                     font-size: 40px;
                     line-height: 47px;
                     display: flex;
                     align-items: space-between;
                     text-transform: capitalize;
                     text-decoration: none;
                     color: #000000;
                     /* Inside auto layout */
                     flex: none;
                     order: 0;
                     flex-grow: 0;
                  }


              </style>

               {{!-- logo, social buttons and later search bar inside header --}}

               <a class="logo{{#if @website.logo}} logo-image{{/if}}" href="{{@website.url}}">
                  <div class="SN_site_HeaderL">
                     <div class="SN_site_logo">
                        {{#if @website.logo}}
                           <img src="{{@website.logo}}" alt="{{@website.name}}" width="{{@website.logoSize.width}}" height="{{@website.logoSize.height}}">
                        {{else}}
                           <strong>{{@website.name}}</strong>
                        {{/if}}
                     </div>
                  </div>
               </a>
               {{#if @config.custom.socialButtons}}
                  <ul class="icons">
                        {{#if @config.custom.socialTwitter}}
                           <li>
                              <a href="{{@config.custom.socialTwitter}}" class="icon brands fa-twitter">
                                 <span class="label">{{ translate 'socials.twitter' }}</span>
                              </a>
                           </li>
                        {{/if}}
                        {{#if @config.custom.socialFacebook}}
                           <li>
                              <a href="{{@config.custom.socialFacebook}}" class="icon brands fa-facebook-f">
                                 <span class="label">{{ translate 'socials.facebook' }}</span>
                              </a>
                           </li>
                        {{/if}}
                        {{#if @config.custom.socialInstagram}}
                           <li>
                              <a href="{{@config.custom.socialInstagram}}" class="icon brands fa-instagram">
                                 <span class="label">{{ translate 'socials.instagram' }}</span>
                              </a>
                           </li>
                        {{/if}}
                        {{#if @config.custom.socialLinkedin}}
                           <li>
                              <a href="{{@config.custom.socialLinkedin}}" class="icon brands fa-linkedin">
                                 <span class="label">{{ translate 'socials.linkedIn' }}</span>
                              </a>
                           </li>
                        {{/if}}
                        {{#if @config.custom.socialVimeo}}
                           <li>
                              <a href="{{@config.custom.socialVimeo}}" class="icon brands fa-vimeo">
                                 <span class="label">{{ translate 'socials.vimeo' }}</span>
                              </a>
                           </li>
                        {{/if}}
                        {{#if @config.custom.socialPinterest}}
                           <li>
                              <a href="{{@config.custom.socialPinterest}}" class="icon brands fa-pinterest">
                                 <span class="label">{{ translate 'socials.pinterest' }}</span>
                              </a>
                           </li>
                        {{/if}}
                        {{#if @config.custom.socialYoutube}}
                           <li>
                              <a href="{{@config.custom.socialYoutube}}" class="icon brands fa-youtube">
                                 <span class="label">{{ translate 'socials.youtube' }}</span>
                              </a>
                           </li>
                        {{/if}}
                  </ul>
               {{/if}}

               {{!-- navbar --}}
               <div class="SN_site_HeaderR">
               {{#checkIfAny menus.mainMenu @config.custom.socialButtons}}
               
                  <nav id="nav">
                     {{#if menus.mainMenu}}
                        {{> menu menus.mainMenu}}
                     {{/if}}
                  </nav>
               {{/checkIfAny}}
               </div>


               {{!-- Hamburger menu --}}
               <div class="S_site_Menu">
                    <div class="S_site_MenuIcon" style="display: block;">
                        <i class="icon-hamburger-menu-3"></i>
                    </div>
                    <div class="S_site_MenuX" style="display: none;">
                        <i class="icon-close-x"></i>
                    </div>
               </div>
            </header>

